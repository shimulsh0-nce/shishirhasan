#include <stdio.h>


int fibonacci(int n) {
    if (n <= 0) {
        return 0; // Invalid input, return 0
    }
    if (n == 1 || n == 2) {
        return 1;
    }

    int a = 1, b = 1, c;
    for (int i = 3; i <= n; i++) {
        c = a + b;
        a = b;
        b = c;
    }
    return b;
}

int main() {
    int n;

    
    printf("Enter the position n: ");
    scanf("%d", &n);

    if (n <= 0) {
        printf("Invalid input. n must be positive.\n");
        return 1;
    }

    
    printf("Fibonacci series up to %dth term: ", n);
    for (int i = 1; i <= n; i++) {
        printf("%d ", fibonacci(i));
    }
    printf("\n");

    return 0;
}
