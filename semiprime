#include <stdio.h>
#include <math.h>

// Function to check if a number is prime
int isPrime(int n) {
    if (n <= 1) return 0;
    if (n == 2) return 1;
    if (n % 2 == 0) return 0;
    for (int i = 3; i <= sqrt(n); i += 2) {
        if (n % i == 0) return 0;
    }
    return 1;
}

// Function to check if a number is semiprime
// Returns 1 if semiprime, 0 otherwise
int isSemiprime(int n) {
    if (n <= 1) return 0; // Semiprimes are greater than 1
    
    // Check for factors
    for (int i = 2; i <= sqrt(n); i++) {
        if (n % i == 0) {
            int factor1 = i;
            int factor2 = n / i;
            // Both factors must be prime
            if (isPrime(factor1) && isPrime(factor2)) {
                return 1;
            }
        }
    }
    return 0;
}

int main() {
    int num;
    
    // Input the number
    printf("Enter a number: ");
    scanf("%d", &num);
    
    // Check and display if it's semiprime
    if (isSemiprime(num)) {
        printf("%d is a semiprime number.\n", num);
    } else {
        printf("%d is not a semiprime number.\n", num);
    }
    
    return 0;
}
